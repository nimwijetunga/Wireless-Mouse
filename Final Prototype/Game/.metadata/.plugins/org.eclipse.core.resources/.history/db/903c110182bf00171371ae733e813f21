import java.awt.Rectangle;

public abstract class Entity extends Rectangle{
	
	protected Rectangle rect = new Rectangle();
	protected int lives,health;
	
	public Entity(int x, int y, int lives, int health){
		
		this.rect.setBounds(x, arg1, arg2, arg3);
		this.lives = lives;
		this.health = health;
	}
	
	public abstract void move();
	
	

}
